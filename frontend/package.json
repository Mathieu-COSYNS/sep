{
  "name": "sep",
  "version": "1.3.0",
  "private": true,
  "author": {
    "name": "Mathieu COSYNS",
    "url": "https://github.com/Mathieu-COSYNS/"
  },
  "type": "module",
  "scripts": {
    "start": "npm run dev",
    "dev": "VITE_APP_VERSION=$npm_package_version vite",
    "build": "npm run typecheck && VITE_APP_VERSION=$npm_package_version vite build",
    "test": "VITE_APP_VERSION=$npm_package_version craco test --transformIgnorePatterns 'node_modules/(?!(@ionic/react|@ionic/react-router|@ionic/core|@stencil/core|ionicons)/)'",
    "preview": "vite preview --port 3000",
    "format": "prettier . --write --ignore-path .gitignore && git update-index --again",
    "lint": "eslint . && stylelint \"**/*.{css,scss}\"",
    "lint:fix": "eslint . --fix; stylelint \"**/*.{css,scss}\" --fix",
    "typecheck": "tsc --noEmit"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@agney/ir-toast": "^0.3.0",
    "@ionic/react": "^6.3.0",
    "@ionic/react-router": "^6.3.0",
    "@reduxjs/toolkit": "^1.8.5",
    "@tanem/react-nprogress": "^5.0.14",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.1.2",
    "@types/node": "^18.8.3",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/react-page-visibility": "^6.4.1",
    "@types/react-qr-reader": "^2.1.4",
    "@types/react-router": "^5.1.19",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-transition-group": "^4.4.5",
    "@vitejs/plugin-legacy": "^3.0.1",
    "@vitejs/plugin-react": "^2.2.0",
    "favicons": "^7.0.1",
    "formik": "^2.2.9",
    "ionicons": "^6.0.3",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-page-visibility": "^7.0.0",
    "react-qr-code": "^2.0.8",
