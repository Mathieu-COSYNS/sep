{
  "name": "sep",
  "version": "2.0.0-beta1",
  "private": true,
  "author": {
    "name": "Mathieu COSYNS",
    "url": "https://github.com/Mathieu-COSYNS/"
  },
  "type": "module",
  "scripts": {
    "start": "npm run dev",
    "dev": "VITE_APP_VERSION=$npm_package_version vite",
    "build": "npm run typecheck && VITE_APP_VERSION=$npm_package_version vite build",
    "test": "vitest",
    "preview": "vite preview --port 3000",
    "format": "prettier . --write",
    "lint": "npm run eslint && npm run stylelint",
    "lint:fix": "npm run eslint -- --fix && npm run stylelint -- --fix",
    "eslint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "stylelint": "stylelint \"**/*.{css,scss}\"",
    "typecheck": "tsc"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@agney/ir-toast": "^0.3.0",
    "@ionic/react": "^7.3.1",
    "@ionic/react-router": "^7.3.1",
    "@reduxjs/toolkit": "^1.9.5",
    "@tanem/react-nprogress": "^5.0.48",
    "@types/lodash": "^4.14.197",
    "@types/node": "^18.17.8",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/react-page-visibility": "^6.4.1",
    "@types/react-qr-reader": "^2.1.4",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-transition-group": "^4.4.6",
    "@vitejs/plugin-legacy": "^4.1.1",
    "@vitejs/plugin-react": "^4.0.4",
    "favicons": "^7.1.4",
    "formik": "^2.4.3",
    "ionicons": "^7.1.2",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-page-visibility": "^7.0.0",
    "react-qr-code": "^2.0.12",
    "react-qr-reader": "^2.2.1",
    "react-redux": "^8.1.2",
    "react-router": "^5.3.4",
    "react-router-dom": "^5.3.4",
    "react-transition-group": "^4.4.5",
    "react-use": "^17.4.0",
    "react-virtuoso": "^4.5.0",
    "sass": "^1.66.1",
    "typescript": "^5.1.6",
    "url": "^0.11.1",
    "vite": "^4.4.9",
    "vite-plugin-favicons-inject": "^2.2.0",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-pwa": "^0.16.4",
    "workbox-core": "^7.0.0",
    "workbox-expiration": "^7.0.0",
    "workbox-precaching": "^7.0.0",
    "workbox-routing": "^7.0.0",
    "workbox-strategies": "^7.0.0",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.1",
    "@testing-library/react": "^14.0.0",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^22.1.0",
    "prettier": "^3.0.2",
    "stylelint": "^15.10.3",
    "stylelint-config-sass-guidelines": "^10.0.0",
    "stylelint-order": "^6.0.3",
    "vitest": "^0.34.2"
  },
  "overrides": {
    "react-qr-reader": {
      "react": "$react",
      "react-dom": "$react-dom"
    }
  }
}
