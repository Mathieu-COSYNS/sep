{
  "github": {
    "silent": true
  },
  "rewrites": [
    {
      "source": "/admin/:path*",
      "destination": "https://sep-prod-backend.herokuapp.com/admin/:path*"
    },
    {
      "source": "/admin/:path*/",
      "destination": "https://sep-prod-backend.herokuapp.com/admin/:path*/"
    },
    {
      "source": "/static/admin/:path*",
      "destination": "https://sep-prod-backend.herokuapp.com/static/admin/:path*"
    }
  ]
}
