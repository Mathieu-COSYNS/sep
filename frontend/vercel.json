{
  "github": {
    "silent": true
  },
  "trailingSlash": true,
  "headers": [
    {
      "source": "*",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "noindex, nofollow"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/admin/",
      "destination": "https://sep-cerclesolvay.herokuapp.com/admin/",
      "has": [
        {
          "type": "host",
          "value": "sep-ten.vercel.app"
        }
      ]
    },
    {
      "source": "/admin/:path*/",
      "destination": "https://sep-cerclesolvay.herokuapp.com/admin/:path*/",
      "has": [
        {
          "type": "host",
          "value": "sep-ten.vercel.app"
        }
      ]
    },
    {
      "source": "/django_lang_switch/:path*/",
      "destination": "https://sep-cerclesolvay.herokuapp.com/django_lang_switch/:path*/",
      "has": [
        {
          "type": "host",
          "value": "sep-ten.vercel.app"
        }
      ]
    },
    {
      "source": "/static/admin/:path*",
      "destination": "https://sep-cerclesolvay.herokuapp.com/static/admin/:path*",
      "has": [
        {
          "type": "host",
          "value": "sep-ten.vercel.app"
        }
      ]
    },
    {
      "source": "/static/django_lang_switch/:path*",
      "destination": "https://sep-cerclesolvay.herokuapp.com/static/django_lang_switch/:path*",
      "has": [
        {
          "type": "host",
          "value": "sep-ten.vercel.app"
        }
      ]
    },
    {
      "source": "/static/admin_plus/:path*",
      "destination": "https://sep-cerclesolvay.herokuapp.com/static/admin_plus/:path*",
      "has": [
        {
          "type": "host",
          "value": "sep-ten.vercel.app"
        }
      ]
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
